<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pro.promark1" >

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE"/>
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH_PRIVILEGED" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateProfile"
            android:label="@string/title_activity_create_profile" >
        </activity>
        <activity
            android:name=".WhenToDo"
            android:label="@string/title_activity_when_to_do" >
        </activity>
        <activity
            android:name=".WiFiTrigger"
            android:label="@string/title_activity_wi_fi_trigger" >
        </activity>
        <activity
            android:name=".WhatToDo"
            android:label="@string/title_activity_what_to_do" >
        </activity>

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" >
        </service>
        <service android:name=".testService" />

        <activity
            android:name=".Headphones"
            android:label="@string/title_activity_headphones" >
        </activity>
        <activity
            android:name=".CellTowerTrigger"
            android:label="@string/title_activity_cell_tower_trigger" >
        </activity>
        <activity
            android:name=".TimeTrigger"
            android:label="@string/title_activity_time_trigger" >
        </activity>
        <activity
            android:name=".BatteryTrigger"
            android:label="@string/title_activity_battery_trigger" >
        </activity>
        <activity
            android:name=".EditProfile"
            android:label="@string/title_activity_edit_profile" >
        </activity>
        <activity
            android:name=".NameProfile"
            android:label="@string/title_activity_name_profile" >
        </activity>
    </application>

</manifest>
